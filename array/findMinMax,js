//max element
let arr=[1,2,4,6,60,700];

let max=arr[0];

for(let i=1;i<arr.length;i++){
    if(max<arr[i]){
        max=arr[i];
    }
}

console.log(max);

console.log(Math.max(...arr));


//min element
let min=arr[0];
for(let i=1;i<arr.length;i++){
    if(min>arr[i]){
        min=arr[i];
    }
}
console.log(min);

console.log(Math.min(...arr));